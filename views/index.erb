Some stuff people made
<form action="location">
  Location: <input type="text" name="location">
</form>

<ul>
<% @repos.each do |repo| %>
  <li>
    <a href="https://github.com/<%= repo.name %>"><%= repo.name %></a> &mdash; 
    <%= repo.description %> 
    (<%= repo.created_at.strftime("%m/%d/%Y") %>)
  </li>
<% end %>
</ul>
